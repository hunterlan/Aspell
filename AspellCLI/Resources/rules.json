{
  "rule": [{
    "typefile": [".c", ".cpp", ".h", ".hpp", ".cs", ".java", ".js", ".ts", ".css"],
    "comments": ["//", "/*br*/"]
  }, {
    "typefile": [".html"],
    "comments": ["<!--br-->"]
  }, {
    "typefile": [".sql"],
    "comments": ["/*br*/", "--"]
  }, {
    "typefile": [".py"],
    "comments": ["#", "\"\"\"br\"\"\""]
  }]
}